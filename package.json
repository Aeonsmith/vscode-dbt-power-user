{
	"name": "vscode-dbt-power-user",
	"displayName": "vscode-dbt-power-user",
	"description": "This extension seamlessly supports DBT within visual code",
  "icon": "media/dbt.png",
	"publisher": "innoverio",
	"license": "MIT",
	"homepage": "https://github.com/innoverio/vscode-dbt-power-user",
	"repository": {
		"type": "git",
		"url": "https://github.com/innoverio/vscode-dbt-power-user.git"
	},
	"bugs": {
		"url": "https://github.com/innoverio/vscode-dbt-power-user/issues"
	},
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"dbt",
		"sql",
		"jinja-sql"
	],
	"activationEvents": [
		"workspaceContains:dbt_project.yml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "run.dbt.currentModel",
				"title": "Run current model",
				"icon": {
					"light": "./media/run-light.svg",
					"dark": "./media/run-dark.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "run.dbt.currentModel",
					"when": "resourceLangId == jinja-sql",
					"group": "1_run"
				}
			]
		},
		"snippets": [
			{
				"language": "jinja-sql",
				"path": "./snippets/snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"lint:fix": "eslint src --ext ts --fix",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"deploy": "vsce publish"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/js-yaml": "^3.12.5",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vsce": "^1.77.0",
		"vscode-test": "^1.3.0"
	},
	"extensionDependencies": [
		"samuelcolvin.jinjahtml"
  ],
  "dependencies": {
		"js-yaml": "^3.14.0"
  }
}
